 <div class="wallDP">
 <%if @user.pic_file_name? %>
        <%= link_to (image_tag @user.pic.url(:wall), class: "img-polaroid"), @user.pic.url %>
        <%else%>
        	<%if @user.gender != "M" %>	
          	     <%= link_to (image_tag("pic_medium_m.png", alt: "Rails")), user_path(current_user)%>
        	<% else %>
        		<%= link_to (image_tag("pic_large_f.png", alt: "Rails", class: "img-polaroid")), user_path(current_user)%>
        	<% end %>

 <%end%>
 </div>
  <div class="pull-left blueText">
    <b><%= link_to "#{current_user.fName} #{current_user.lName}", user_path(@user) %><br></b>
    <%= @user.email%><br>
 </div>