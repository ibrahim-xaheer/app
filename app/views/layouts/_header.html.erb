<header class="navbar navbar-fixed-top">
  <div class="navbar-inner darkBlueBG">
    <div class="container">
      <div class="pull-left">
        <%= link_to logo, root_path %>
      </div>
      <nav>
        <ul class="nav pull-right grayText">

          
         
          <% if signed_in? %>          
          <li class="grayText"><%= link_to "Home", user_path(current_user) %></li>
          <li><%= link_to "Users", users_path %></li>
          <li><%= link_to "Settings", edit_user_path(current_user) %></li>
          <li><%= link_to "Sign out", signout_path, method: "delete" %></li>
        </ul>
            
          <% else %>
          <li class="grayText"><%= link_to "Home", root_path %></li>
           <li class="grayText"><%= link_to "Help", about_path %></li>
            <li><%= link_to "Sign in", signin_path %></li>
          <% end %>
        
        </ul>
      </nav>
    </div>
  </div>
</header>